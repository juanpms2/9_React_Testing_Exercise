{
  "name": "testing-exercise",
  "version": "1.0.0",
  "description": "Práctica del módulo 9 del máster. Testing en React con Jest y Cypress.",
  "main": "index.js",
  "scripts": {
    "start": "run-p -l start:dev start:mock-server",
    "start:dev": "webpack-dev-server",
    "start:mock-server": "cd ./server && npm start",
    "postinstall": "cd ./server && npm i",
    "test": "jest --verbose -c ./config/test/jest.json",
    "test:watch": "npm test -- --watchAll -i --no-cache",
    "pretest:coverage": "rimraf coverage",
    "test:coverage": "jest --verbose -c ./config/test/jest.coverage.json"
  },
  "author": "Juan Pablo Martínez",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "deep-freeze": "0.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.1.1",
    "typescript": "3.8.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@lemoncode/fonk": "^1.3.0",
    "@lemoncode/fonk-final-form": "^2.3.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "final-form": "^4.20.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.3.5",
    "react-router-dom": "^5.1.2"
  }
}
